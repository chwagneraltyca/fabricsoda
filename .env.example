# Copy to .env and fill in your values
# DQ Checker workspace configuration

# Service Principal (needs Contributor access to workspace)
AZURE_CLIENT_ID=<app-registration-client-id>
AZURE_CLIENT_SECRET=<app-registration-secret>
AZURE_TENANT_ID=<tenant-id>

# Fabric workspace for DevGateway
DQ_WORKSPACE_ID=<workspace-guid>

# Target Data Warehouse (source for DQ checks)
DQ_DWH_SERVER=<your-workspace>.datawarehouse.fabric.microsoft.com
DQ_DWH_DATABASE=<warehouse-name>

# Metadata SQL Database (stores check definitions, results)
DQ_SQL_SERVER=<your-workspace>.database.fabric.microsoft.com,1433
DQ_SQL_DATABASE=<database-name>

# GraphQL API endpoint
DQ_GRAPHQL_ENDPOINT=https://<workspace-id>.graphql.fabric.microsoft.com/v1/workspaces/<workspace-id>/graphqlapis/<api-id>/graphql
